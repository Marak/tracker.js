{
    "name" : "dnode",
    "version" : "0.6.10",
    "description" : "freestyle RPC",
    "main" : "./index.js",
    "keywords" : [
        "message passing",
        "rpc",
        "rmi",
        "drb",
        "remote",
        "communication",
        "websockets",
        "socket.io"
    ],
    "repository" : {
        "type" : "git",
        "url" : "http://github.com/substack/dnode.git"
    },
    "directories" : {
        "lib" : ".",
        "examples" : "./examples"
    },
    "dependencies" : {
        "socket.io" : ">=0.6.1 <0.7",
        "lazy" : ">=1.0.2 <1.1",
        "traverse" : ">=0.3.1 <0.4",
        "browserify" : ">=0.3.6 <0.4",
        "dnode-protocol" : ">=0.0.8"
    },
    "bundleDependencies" : [ "dnode-protocol" ],
    "browserify" : {
        "name" : "dnode",
        "main" : "./browser/index.js",
        "base" : "./browser",
        "require" : [ "dnode-protocol" ]
    },
    "engine" : {
        "node" : ">=0.4.0"
    },
    "author" : {
        "name" : "James Halliday",
        "email" : "mail@substack.net",
        "url" : "http://substack.net"
    }
}
